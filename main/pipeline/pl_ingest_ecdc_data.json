{
	"name": "pl_ingest_ecdc_data",
	"properties": {
		"description": "This will ingest all the ECDC data using the HTTP protocol.",
		"activities": [
			{
				"name": "Lookup ecdc file list",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"dataset": {
						"referenceName": "ds_ecdc_files_list",
						"type": "DatasetReference",
						"parameters": {
							"sourceBasseURL": {
								"value": "@variables('sourceURL')",
								"type": "Expression"
							},
							"sourceRelativeURL": {
								"value": "@variables('relativeURL')",
								"type": "Expression"
							}
						}
					},
					"firstRowOnly": false
				}
			}
		],
		"variables": {
			"relativeURL": {
				"type": "String"
			},
			"sourceURL": {
				"type": "String"
			}
		},
		"annotations": []
	}
}